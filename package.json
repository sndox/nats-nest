{
  "name": "@sndo/nats-nest",
  "version": "0.3.0",
  "description": "A lightweight wrapper around an embedded NATS server",
  "type": "module",
  "bin": "./dist/nats-nest",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:go": "cd ./go && go build -o ../dist/nats-nest .",
    "build:js": "rimraf ./dist && tsc",
    "build:types": "tsc -d --declarationDir dist --emitDeclarationOnly",
    "build": "pnpm run build:js && pnpm run build:types && pnpm run build:go",
    "dev": "go run ./main.go --host 127.0.0.1",
    "prepublishOnly": "pnpm run build",
    "test": "exit 0"
  },
  "packageManager": "pnpm@8.14.2",
  "repository": {
    "url": "https://github.com/sndox/nats-nest.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "NATS",
    "server",
    "wrapper",
    "embedded",
    "Go"
  ],
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "useTabs": false,
    "endOfLine": "auto",
    "singleAttributePerLine": false,
    "bracketSameLine": false,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "semi": false
  },
  "dependencies": {
    "execa": "^9.3.1"
  }
}
