{
  "name": "@sndo/nats-nest",
  "version": "0.1.0",
  "description": "A lightweight wrapper around an embedded NATS server",
  "type": "module",
  "bin": "./dist/nats-nest",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "go build -o ./dist/nats-nest .",
    "dev": "go run ./main.go --host 127.0.0.1",
    "prepublishOnly": "pnpm run build",
    "test": "exit 0"
  },
  "packageManager": "pnpm@8.14.2",
  "repository": {
    "url": "https://github.com/sndox/nats-nest.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "NATS",
    "server",
    "wrapper",
    "embedded",
    "Go"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.5.4"
  }
}
